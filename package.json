  "license": "ISC",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "antd": "^4.9.2",
    "audiobuffer-to-wav": "^1.0.0",
    "aws-sdk": "^2.838.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "ffmpeg": "0.0.4",
    "file-type": "^16.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "speaker": "^0.5.2",
    "supports-color": "^7.1.0",
    "tmp": "^0.2.1",
    "wav-file-info": "0.0.10",
    "web-audio-api": "^0.2.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-rc.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
